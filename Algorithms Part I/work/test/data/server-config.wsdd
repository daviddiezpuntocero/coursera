<deployment   xmlns="http://xml.apache.org/axis/wsdd/"   xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">   
    <handler   type="java:org.apache.axis.handlers.http.URLMapper"   name="URLMapper"/>   
    <service   name="ESPSupplierTriggerService"   provider="java:RPC">   
        <parameter   name="className"   value="com.cnooc.integration.MockESPSupplierTriggerService"/>
        <parameter name="allowedMethods" value="SupplierTrigger"/>   
    </service>   
    <transport   name="http">   
        <requestFlow>   
            <handler   type="URLMapper"/>   
        </requestFlow>   
    </transport>   
</deployment>   
